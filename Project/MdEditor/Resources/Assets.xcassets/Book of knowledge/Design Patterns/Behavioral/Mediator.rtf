// \uc0\u1055 \u1088 \u1086 \u1090 \u1086 \u1082 \u1086 \u1083  \u1076 \u1083 \u1103  \u1086 \u1073 \u1097 \u1077 \u1075 \u1086  \u1087 \u1086 \u1074 \u1077 \u1076 \u1077 \u1085 \u1080 \u1103  \u1087 \u1086 \u1083 \u1100 \u1079 \u1086 \u1074 \u1072 \u1090 \u1077 \u1083 \u1077 \u1081 \
\
protocol User \{\
\
\'a0 \'a0 var name: String \{ get \}\
\
\'a0 \'a0 func sendMessage(message: String)\
\
\'a0 \'a0 func receiveMessage(message: String)\
\
\}\
\
  \
\
// \uc0\u1050 \u1086 \u1085 \u1082 \u1088 \u1077 \u1090 \u1085 \u1099 \u1081  \u1087 \u1086 \u1083 \u1100 \u1079 \u1086 \u1074 \u1072 \u1090 \u1077 \u1083 \u1100 \
\
class ConcreteUser: User \{\
\
\'a0 \'a0 let name: String\
\
\'a0 \'a0 let mediator: ChatMediator\
\
\'a0 \'a0 init(name: String, mediator: ChatMediator) \{\
\
\'a0 \'a0 \'a0 \'a0 self.name = name\
\
\'a0 \'a0 \'a0 \'a0 self.mediator = mediator\
\
\'a0 \'a0 \}\
\
\'a0 \'a0 func sendMessage(message: String) \{\
\
\'a0 \'a0 \'a0 \'a0 mediator.sendMessage(message: message, sender: self)\
\
\'a0 \'a0 \}\
\
\'a0 \'a0 func receiveMessage(message: String) \{\
\
\'a0 \'a0 \'a0 \'a0 print("\\(name) received message: \\(message)")\
\
\'a0 \'a0 \}\
\
\}\
\
  \
\
// \uc0\u1055 \u1086 \u1089 \u1088 \u1077 \u1076 \u1085 \u1080 \u1082 \
\
class ChatMediator \{\
\
\'a0 \'a0 private var users = [User]()\
\
\'a0 \'a0 func addUser(user: User) \{\
\
\'a0 \'a0 \'a0 \'a0 users.append(user)\
\
\'a0 \'a0 \}\
\
\'a0 \'a0 func sendMessage(message: String, sender: User) \{\
\
\'a0 \'a0 \'a0 \'a0 for user in users \{\
\
\'a0 \'a0 \'a0 \'a0 \'a0 \'a0 if user !== sender \{\
\
\'a0 \'a0 \'a0 \'a0 \'a0 \'a0 \'a0 \'a0 user.receiveMessage(message: message)\
\
\'a0 \'a0 \'a0 \'a0 \'a0 \'a0 \}\
\
\'a0 \'a0 \'a0 \'a0 \}\
\
\'a0 \'a0 \}\
\
\}\
\
